<h1>Listing People</h1>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
        </tr>
    </thead>

    <tbody>
        <% @people.each do |person| %>
            <tr>
                <td> <%= person.name %> </td>
                <td> <%= person.age %> </td>
            </tr>
        <% end %>
    </tbody>
</table>